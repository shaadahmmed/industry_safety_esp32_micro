generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sensorData {
  id        String   @id @default(uuid())
  flameRead Int
  tempRead  Float
  humidity  Float
  gasRead   Float
  timestamp DateTime @default(now())
}
